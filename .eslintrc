{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "import/prefer-default-export": 0,
    "jsx-a11y/img-has-alt": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "react/no-danger": 0,
    "no-constant-condition": 0,
    "no-alert": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-tabs": 0,
    "no-underscore-dangle": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.dev.babel.js"
      }
    }
  },
  "globals": {
    "document": true,
    "window": true,
    "google": true
  }
}
